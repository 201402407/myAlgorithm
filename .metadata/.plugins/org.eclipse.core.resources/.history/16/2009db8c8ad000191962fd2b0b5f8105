package dfsnfs;

import java.util.Scanner;

// BFS 탐색
public class p1697 {
	static int[] map;
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		// n과 k값 받기
		int n = sc.nextInt();
		if(n < 0 || n > 100000) {
			System.exit(0);
		}
		int k = sc.nextInt();
		if(k < 0 || k > 100000) {
			System.exit(0);
		}
		
		// map 배열 크기 설정
		if(n >= k) {
			map = new int[n];
		}
		if(n < k) {
			map = new int[2 * k];
		}
		
		// BFS 시작
	}
	
	private static int findBrotherOrSister() {
		
	}
}
